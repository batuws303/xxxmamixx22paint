-- Functions --

-- ULTRA DEFENSE SYSTEM --
local function startUltraDefense()
	if ultraDefenseConnection then
		ultraDefenseConnection:Disconnect()
	end
	
	ultraDefenseConnection = task.spawn(function()
		while ultraFastMode do
			-- Tüm korunan province'leri sürekli kontrol et
			for _, info in pairs(provinces) do
				if info.Province and info.Province.Parent then
					-- Eğer rengi değiştiyse ANINDA geri boyama
					if info.Province.Color ~= color then
						-- 5 KEZ BOYAMA SALDIRISI
						for i = 1, 5 do
							if player and player.Character and player.Character:FindFirstChild("PaintBucket") then
								local args = {
									[1] = "PaintPart",
									[2] = {
										["Part"] = info.Province,
										["Color"] = color
									},
									[3] = "Peace"
								}
								pcall(function()
									game:GetService("Players").LocalPlayer.Character.PaintBucket.Remotes.ServerControls:InvokeServer(unpack(args))
								end)
							end
						end
						provincespersecond = provincespersecond + 5
					end
				end
			end
			-- HİÇ BEKLEME - MAKSIMUM HIZ
		end
	end)
end

-- DRAG SELECT FUNCTIONS --
local function startDragSelect()
	if dragConnection then
		dragConnection:Disconnect()
	end
	
	dragConnection = mouse.Move:Connect(function()
		if isDragging and dragSelectMode then
			if mouse.Target and mouse.Target.Name == "Province" then
				local alreadySelected = false
				
				for _, info in pairs(provinces) do
					if info.Province == mouse.Target then
						alreadySelected = true
						break
					end
				end
				
				if not alreadySelected then
					for _, selected in pairs(selectedInDrag) do
						if selected == mouse.Target then
							alreadySelected = true
							break
						end
					end
				end
				
				if not alreadySelected then
					table.insert(selectedInDrag, mouse.Target)
					
					provinceSound:Play()
					
					local clone = mouse.Target:Clone()
					clone.Position -= Vector3.new(0, 0.01, 0)
					clone.Material = "Glass"
					clone.Transparency = 1000000
					clone.Parent = provincesFolder
					
					local coro = coroutine.create(paintProvinceLoop)
					coroutine.resume(coro, mouse.Target, coro, clone)
					
					local tempHighlight = Instance.new("SelectionBox")
					tempHighlight.Adornee = mouse.Target
					tempHighlight.Color3 = Color3.fromRGB(0, 255, 0)
					tempHighlight.Parent = mouse.Target
					tempHighlight.Transparency = 0.3
					
					task.spawn(function()
						task.wait(0.5)
						if tempHighlight then
							tempHighlight:Destroy()
						end
					end)
				end
			end
		end
	end)
end

local function finishDragSelect()
	if dragConnection then
		dragConnection:Disconnect()
		dragConnection = nil
	end
	
	if #selectedInDrag > 0 then
		print("Added " .. #selectedInDrag .. " provinces via drag select!")
	end
	
	selectedInDrag = {}
	isDragging = false
end

-- ORIJINAL paintProvinceLoop (HIZLANDIRILMIŞ) --
local function paintProvinceLoop(province,coro,highlighty)
	local savedColor = color
	local active = true

	local eventObject = {}
	function eventObject:Disconnect()
		active = false
	end

	task.spawn(function()
		while active do
			if painting then
				if player and player.Character and player.Character:FindFirstChild("PaintBucket") then
					if (province.Color ~= color and keepTerColor == false) or (keepTerColor == true and province.Color ~= savedColor) then
						local args = {
							[1] = "PaintPart",
							[2] = {
								["Part"] = province,
								["Color"] = keepTerColor and savedColor or color
							},
							[3] = "Peace"
						}
						
						-- ULTRA FAST MODUNDA ÇOKLU BOYAMA
						if ultraFastMode then
							-- 3 KEZ BOYAMA SALDIRISI
							for i = 1, 3 do
								pcall(function()
									game:GetService("Players").LocalPlayer.Character.PaintBucket.Remotes.ServerControls:InvokeServer(unpack(args))
								end)
							end
							provincespersecond = provincespersecond + 3
						else
							pcall(function()
								game:GetService("Players").LocalPlayer.Character.PaintBucket.Remotes.ServerControls:InvokeServer(unpack(args))
							end)
							provincespersecond = provincespersecond + 1
						end
					end
				end
			end

			-- HIZ KONTROLLARI
			if safeMode then
				task.wait(0.3)
			elseif ultraFastMode then
				-- HİÇ BEKLEME - MAKSIMUM SALDIRI
			elseif newpaint then
				task.wait(0.01)
			else
				task.wait(0.02)
			end
		end
	end)

	local provinceInfo = {
		["Province"] = province,
		["Coroutine"] = coro,
		["Event"] = eventObject,
		["Highlight"] = highlighty
	}
	table.insert(provinces,provinceInfo)
	provinceCodeGenerator += 1
end

local function createNewOption(optionType,text,customText,enabled)
	local optionFrame = Instance.new("Frame",scrollFrame)
	optionFrame.BorderSizePixel = 0
	optionFrame.BackgroundColor3 = Color3.fromRGB(32,34,37) -- Modern card rengi
	optionFrame.Size = UDim2.new(1,0,0,45) -- Sabit pixel boyut
	optionFrame.Parent = scrollFrame
	-- Modern köşe yuvarlama
	local frameCorner = Instance.new("UICorner",optionFrame)
	frameCorner.CornerRadius = UDim.new(0,8)
	
	local textlabel = Instance.new("TextLabel",optionFrame)
	textlabel.BackgroundTransparency = 1
	textlabel.Position = UDim2.new(0,12,0,0)
	textlabel.Size = UDim2.new(0.6,0,1,0)
	textlabel.TextSize = 14 -- Sabit text boyutu
	textlabel.Text = text
	textlabel.Font = Enum.Font.Gotham
	textlabel.TextColor3 = Color3.fromRGB(220,220,220)
	textlabel.TextXAlignment = Enum.TextXAlignment.Left
	textlabel.TextYAlignment = Enum.TextYAlignment.Center
	
	if optionType == "switch" then
		local switchFrame = Instance.new("Frame",optionFrame)
		switchFrame.BackgroundColor3 = Color3.fromRGB(60,60,60)
		switchFrame.Position = UDim2.new(1,-60,0.5,-10)
		switchFrame.Size = UDim2.new(0,50,0,20) -- Küçük modern switch
		switchFrame.BorderSizePixel = 0
		local switchCorner = Instance.new("UICorner",switchFrame)
		switchCorner.CornerRadius = UDim.new(0,10)
		
		local switch = Instance.new("ImageButton",switchFrame)
		switch.AutoButtonColor = false
		switch.BorderSizePixel = 0
		switch.Size = UDim2.new(0,18,0,18)
		switch.Position = UDim2.new(0,1,0,1)
		switch.BackgroundColor3 = Color3.fromRGB(255,255,255)
		local switchButtonCorner = Instance.new("UICorner",switch)
		switchButtonCorner.CornerRadius = UDim.new(0,9)
		
		if enabled then
			switchFrame.BackgroundColor3 = Color3.fromRGB(0,122,255) -- Modern mavi
			switch.Position = UDim2.new(1,-19,0,1)
		end
		return switch
	elseif optionType == "color" then
		local colorButton = Instance.new("ImageButton",optionFrame)
		colorButton.Position = UDim2.new(1,-35,0.5,-12)
		colorButton.AutoButtonColor = false
		colorButton.Size = UDim2.new(0,24,0,24) -- Küçük color button
		colorButton.BackgroundColor3 = color
		local colorCorner = Instance.new("UICorner",colorButton)
		colorCorner.CornerRadius = UDim.new(0,6)
		return colorButton
	elseif optionType == "custom" then
		local button = Instance.new("TextButton",optionFrame)
		button.BackgroundColor3 = Color3.fromRGB(0,122,255) -- Modern mavi
		button.Position = UDim2.new(1,-70,0.5,-12)
		button.Size = UDim2.new(0,60,0,24) -- Küçük button
		button.AutoButtonColor = false
		button.BorderSizePixel = 0
		button.Text = customText
		button.TextSize = 12
		button.Font = Enum.Font.GothamMedium
		button.TextColor3 = Color3.fromRGB(255,255,255)
		local buttonCorner = Instance.new("UICorner",button)
		buttonCorner.CornerRadius = UDim.new(0,6)
		return button
	end
end

local function update(input)
	local delta = input.Position - dragStart
	mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

